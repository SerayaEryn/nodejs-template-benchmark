<html>
  <head>
    <title>{{ title | safe }}</title>
  </head>
  <body>
    <p>{{ text | safe }}</p>
    {% for project in projects %}
      <a href="{{ project.url | safe }}">{{ project.name | safe }}</a>
      <p>{{ project.description | safe }}</p>
    {% else %}
      No projects
    {% endfor %}
    </if>
  </body>
</html>
